<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <!--Debug : 디버그 모드에서는 SMS를 보내지 않는 기능 Y:SMS 전송 X / N:SMS 전송 O-->
    <add key="Debug" value="Y"/>
    
    <!--SearchDrive : 띄어쓰기 없이 | 로 파일 체크할 드라이브 구분-->
    <add key="SearchDrive" value="C:\|D:\" />
    
    <add key="HostName" value="FRONTSERVERHOSTNAME" />
   
    
    <!--ExceptionWord : 띄어쓰기 없이 | 로 단어 구분하여 추가-->
    <add key="ExceptionWord" value="windows|system volume information|log" />
    
    <!--PhoneNum : 띄어쓰기 없이 | 로 전화번호 추가-->
   
    <add key="PhoneNum" value="" />

    <add key="CallBackNum" value="" />
    
    <add key="PartCode" value="T" />
  </appSettings>
  
</configuration>